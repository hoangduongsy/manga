<h1>Mangas</h1>

<table class="table table-hover">

<thead><tr><td>Series Name:</td><td>Rating:</td><td>Admin:</td></tr></thead>

<% @mangas.each do |manga| %> 

	<tr>
		<td><%= link_to manga.name, manga_path(manga) %></td>
		<td></td>
		<td><%= link_to "Manage", edit_manga_path(manga.slug) %></td>
	</tr>

<% end %>

</table>
